<nav class="linklist">
	<% for document in @getCollection('posts').toJSON(): %>
		<li>
			<a href="<%= document.url %>"><%= document.title %></a>
			&raquo;
			<span><%= document.postDate %></span>
		</li>
	<% end %>
</nav>
