<h3>News</h3>
<nav class="linklist">
	<% for document in @getCollection('news').toJSON(): %>
		<li>
			<a href="<%= document.url %>"><%= document.title %></a>
			<% if document.postDate: %>
				&raquo;
				<span><%= document.postDate %></span>
			<% end %>
			<% if document.desc: %>
				<br>
				<sub><%= document.desc %></sub>
			<% end %>
		</li>
		<br>
	<% end %>
</nav>

<br>
